      <version>42.1.4.jre7</version>
    </dependency>
    <dependency>
	   <groupId>ca.uhn.hapi.fhir</groupId>
	   <artifactId>hapi-fhir-structures-dstu3</artifactId>
	   <version>3.0.0</version>
	</dependency>
    <dependency>
   		<groupId>ca.uhn.hapi.fhir</groupId>
